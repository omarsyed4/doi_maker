<Window
    
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:toolkit="http://schemas.microsoft.com/wpf/2008/toolkit" xmlns:x1="clr-namespace:System;assembly=System.Runtime"
        x:Class="WpfApp1.MainWindow"
        mc:Ignorable="d"
    


        Title="AA DOI MAKER" Height="2162" Width="1140"
    
        KeyDown="Window_KeyDown"
    
        Background="#262A2D">

    <Grid x:Name="mainGrid">


        <ScrollViewer x:Name="ScrollBar"  HorizontalScrollBarVisibility="Disabled" Margin="0,0,5,0">


            <Grid Height="2166" Margin="0,0,0,712">


                <TextBlock HorizontalAlignment="Left" Margin="10,16,0,0" TextWrapping="Wrap" Text="AA DOI MAKER" VerticalAlignment="Top" FontFamily="Bahnschrift" FontSize="18" Height="22" Width="149" Foreground="#FAF9F7"/>



                <StackPanel Orientation="Vertical" Margin="10,43,894,0" Height="52" VerticalAlignment="Top">
                    <TextBlock Text="Date Of The Incident?" FontFamily="Bahnschrift" Foreground="#FAF9F7"/>
                    <StackPanel Orientation="Horizontal">
                        <Border Background="#2E3235" BorderThickness="1" BorderBrush="#2E3235" CornerRadius="5" Margin="0,5,5,5" Width="165" Height="25">
                            <DatePicker x:Name="DateOfIncident" FontFamily="Bahnschrift" SelectedDateChanged="dateofincident_SelectedDateChanged" Background="Transparent" BorderThickness="0" Padding="0" Margin="2,2,2,2" Height="19"/>
                        </Border>
                        <CheckBox x:Name="ApproxDateOfIncident" Content="Approx?" FontFamily="Bahnschrift" Margin="5,0,0,0" Foreground="White" Visibility="Collapsed" Height="15" Checked="ApproxDateOfIncident_Checked" Unchecked="ApproxDateOfIncident_Unchecked"/>
                    </StackPanel>
                </StackPanel>


                <StackPanel HorizontalAlignment="Left" Margin="0,-7,0,0" Width="344" Height="183" VerticalAlignment="Top">
                    <TextBlock HorizontalAlignment="Left" Margin="10,111,0,0" TextWrapping="Wrap" Text="City and State of The Incident?" VerticalAlignment="Top" FontFamily="Bahnschrift" Height="14" Width="161" Foreground="#FAF9F7"/>
                    <StackPanel Orientation="Horizontal" RenderTransformOrigin="0.532,0.527" Width="322" Height="54">
                        <TextBlock Text="City: " Height="20" FontFamily="Bahnschrift" RenderTransformOrigin="13.944,1.001" Foreground="#FAF9F7"/>
                        <Border Background="#2E3235" BorderThickness="1" BorderBrush="#2E3235" CornerRadius="5" Height="22" Width="118">
                            <TextBox x:Name="CityofIncident" CaretBrush="White" FontFamily="Bahnschrift" Text="" Foreground="#FAF9F7" HorizontalAlignment="Center" Width="120" TextWrapping="Wrap" Background="Transparent" BorderThickness="0" Padding="5" Margin="0,-2,0,0" TextChanged="CityTextBox_TextChanged_1"/>
                        </Border>

                        <TextBlock Text=" State: " Height="20" FontFamily="Bahnschrift" RenderTransformOrigin="0.413,0.568" TextWrapping="Wrap" Width="35" IsEnabled="False" Foreground="#FAF9F7"/>
                        <ComboBox x:Name="StateOfIncident" Background="#2E3235" Width="141" Height="20" FontFamily="Bahnschrift" SelectionChanged="StateOfIncident_SelectionChanged" >
                            <ComboBoxItem Content="Alabama" />
                            <ComboBoxItem Content="Alaska" />
                            <ComboBoxItem Content="Arizona" />
                            <ComboBoxItem Content="Arkansas" />
                            <ComboBoxItem Content="California" />
                            <ComboBoxItem Content="Colorado" />
                            <ComboBoxItem Content="Connecticut" />
                            <ComboBoxItem Content="Delaware" />
                            <ComboBoxItem Content="District Of Columbia" />
                            <ComboBoxItem Content="Florida" />
                            <ComboBoxItem Content="Georgia" />
                            <ComboBoxItem Content="Hawaii" />
                            <ComboBoxItem Content="Idaho" />
                            <ComboBoxItem Content="Illinois" />
                            <ComboBoxItem Content="Indiana" />
                            <ComboBoxItem Content="Iowa" />
                            <ComboBoxItem Content="Kansas" />
                            <ComboBoxItem Content="Kentucky" />
                            <ComboBoxItem Content="Louisiana" />
                            <ComboBoxItem Content="Maine" />
                            <ComboBoxItem Content="Maryland" />
                            <ComboBoxItem Content="Massachusetts" />
                            <ComboBoxItem Content="Michigan" />
                            <ComboBoxItem Content="Minnesota" />
                            <ComboBoxItem Content="Mississippi" />
                            <ComboBoxItem Content="Missouri" />
                            <ComboBoxItem Content="Montana" />
                            <ComboBoxItem Content="Nebraska" />
                            <ComboBoxItem Content="Nevada" />
                            <ComboBoxItem Content="New Hampshire" />
                            <ComboBoxItem Content="New Jersey" />
                            <ComboBoxItem Content="New Mexico" />
                            <ComboBoxItem Content="New York" />
                            <ComboBoxItem Content="North Carolina" />
                            <ComboBoxItem Content="North Dakota" />
                            <ComboBoxItem Content="Ohio" />
                            <ComboBoxItem Content="Oklahoma" />
                            <ComboBoxItem Content="Oregon" />
                            <ComboBoxItem Content="Pennsylvania" />
                            <ComboBoxItem Content="Rhode Island" />
                            <ComboBoxItem Content="South Carolina" />
                            <ComboBoxItem Content="South Dakota" />
                            <ComboBoxItem Content="Tennessee" />
                            <ComboBoxItem Content="Texas" />
                            <ComboBoxItem Content="Utah" />
                            <ComboBoxItem Content="Vermont" />
                            <ComboBoxItem Content="Virginia" />
                            <ComboBoxItem Content="Washington" />
                            <ComboBoxItem Content="West Virginia" />
                            <ComboBoxItem Content="Wisconsin" />
                            <ComboBoxItem Content="Wyoming" />
                        </ComboBox>
                    </StackPanel>
                </StackPanel>

                <StackPanel Orientation="Horizontal" Margin="0,365,659,0" Height="18" VerticalAlignment="Top">
                    <TextBlock Text="Type of accident: " FontFamily="Bahnschrift" FontSize="14" Foreground="White" Margin="10,0,10,0"/>
                    <ComboBox x:Name="TypeOfAccident" FontFamily="Bahnschrift" Width="200" SelectionChanged="TypeOfAccident_SelectionChanged">
                        <ComboBoxItem Content="Hit &amp; Run"/>
                        <ComboBoxItem Content="Multi Car"/>
                        <ComboBoxItem Content="Pedestrian/Vehicle"/>
                        <ComboBoxItem Content="Cyclist/Vehicle"/>
                        <ComboBoxItem Content="Single Vehicle"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Margin="10,176,600,0" Height="172" VerticalAlignment="Top">
                    <TextBlock Text="IP's role:" FontFamily="Bahnschrift" FontSize="14" Width="60" Foreground="White" Height="16" HorizontalAlignment="Left"/>
                    <StackPanel Orientation="Horizontal">
                        <RadioButton x:Name="IPWasDriver" Content="Driver" FontFamily="Bahnschrift" Margin="0,5" Foreground="White" Checked="IPWasDriver_Checked"/>
                        <RadioButton x:Name="IPWasMassTransitPassenger" Content="Mass Transit" FontFamily="Bahnschrift" Margin="5" Foreground="White" Checked="IPWasMassTransitPassenger_Checked" Unchecked="IPWasMassTransitPassenger_Unchecked"/>
                        <RadioButton x:Name="IPWasPassenger" Content="Passenger" FontFamily="Bahnschrift" Margin="5" Foreground="White" Checked="IPWasPassenger_Checked"/>
                        <RadioButton x:Name="IPWasPedestrian" Content="Pedestrian" FontFamily="Bahnschrift" Margin="5" Foreground="White" Checked="IPWasPedestrian_Checked"/>
                        <RadioButton x:Name="IPWasCyclist" Content="Cyclist" FontFamily="Bahnschrift" Margin="5" Foreground="White" Checked="IPWasCyclist_Checked"/>
                    </StackPanel>
                    <StackPanel x:Name="DriverFUQuestions" Visibility="Collapsed" Orientation="Vertical" Width="533" HorizontalAlignment="Left" Margin="0,5,0,0">
                        <StackPanel x:Name="DriverFUQuestion1" Visibility="Collapsed" Margin="0,0,5,5" Height="60" Width="352" HorizontalAlignment="Left">
                            <TextBlock Text="Were there any passengers in the car at the time?" FontFamily="Bahnschrift" FontSize="14" Margin="0,0,0,5" Foreground="White"/>
                            <RadioButton x:Name="DriverFUQuestion1Yes" Content="Yes" FontFamily="Bahnschrift" HorizontalAlignment="Left" Margin="0,3" Foreground="White" Width="40" Checked="DriverFUQuestion1Yes_Checked"/>
                            <RadioButton x:Name="DriverFUQuestion1No" Content="No" FontFamily="Bahnschrift" HorizontalAlignment="Left" Margin="0,3" Foreground="White" Width="40" Checked="DriverFUQuestion1No_Checked"/>
                        </StackPanel>
                        <StackPanel x:Name="DriverFUQuestion2"  Visibility="Collapsed" Height="62" Width="239" HorizontalAlignment="Left">
                            <TextBlock Text="Who were they in relation to IP:" FontFamily="Bahnschrift" FontSize="14" Foreground="White"/>
                            <Border BorderBrush="#2E3235" BorderThickness="2" Background="#2E3235" CornerRadius="10" Margin="0,5" Width="235" HorizontalAlignment="Left" Height="35">
                                <TextBox x:Name="IPDriverPassengerRelations" CaretBrush="White" FontFamily="Bahnschrift" BorderThickness="0" Background="Transparent" Foreground="White" Padding="5" HorizontalAlignment="Center" VerticalAlignment="Center" Width="214"/>
                            </Border>
                        </StackPanel>

                    </StackPanel>
                    <StackPanel x:Name="PassengerFUQuestion1" Orientation="Horizontal"  Visibility="Collapsed" Width="518" HorizontalAlignment="Left" Height="123">
                        <StackPanel  Visibility="Visible" HorizontalAlignment="Left" Margin="0,0,10,0">
                            <TextBlock Text="Driver's Relation to IP:" FontFamily="Bahnschrift" FontSize="14" Margin="0,5,0,0" Foreground="White"/>
                            <Border BorderBrush="#2E3235" BorderThickness="2" Background="#2E3235" CornerRadius="10" Width="200" HorizontalAlignment="Left">
                                <TextBox x:Name="IPPassengerDriverRelations" CaretBrush="White" FontFamily="Bahnschrift" Width="190" BorderThickness="0" Background="Transparent" Foreground="White" Padding="5" Height="25"/>
                            </Border>
                            <StackPanel Height="63" Margin="0,7,0,45" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                                <TextBlock Text="Were there any other passengers in the car at the time?" FontFamily="Bahnschrift" FontSize="14" Margin="0,0,0,5" Foreground="White"/>
                                <StackPanel Orientation="Vertical">
                                    <RadioButton x:Name="IPPassengerPassengersOtherThanDriver" Content="Yes" FontFamily="Bahnschrift" HorizontalAlignment="Left" Margin="0,3" Foreground="White" Width="40" Checked="RadioButton_Checked" />
                                    <RadioButton x:Name="IPPassengerPassengersOtherThanDriverNO" Content="No" FontFamily="Bahnschrift" HorizontalAlignment="Left" Margin="0,3" Foreground="White" Width="40" Checked="RadioButton_Checked_1"/>
                                </StackPanel>

                            </StackPanel>

                        </StackPanel>
                        <StackPanel x:Name="PassengerFUQuestion2"  Visibility="Collapsed" Width="146" Height="79">
                            <TextBlock Text="Who were they in relation to IP:" TextWrapping="Wrap" FontFamily="Bahnschrift" FontSize="14" Margin="0,5,0,0" Foreground="White" Width="109" HorizontalAlignment="Center"/>
                            <Border BorderBrush="#2E3235" BorderThickness="2" Background="#2E3235" CornerRadius="10" Width="146" HorizontalAlignment="Left" Height="36">
                                <TextBox x:Name="IPPassengerOtherPassengerRelations" CaretBrush="White" FontFamily="Bahnschrift" BorderThickness="0" Background="Transparent" Foreground="White" Padding="5" Margin="3,-2,3,-2"/>
                            </Border>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel x:Name="MassTransitFUQuestion1"  Visibility="collapsed" Width="201" HorizontalAlignment="Left">
                        <TextBlock Text="Public transport vehicle:" FontFamily="Bahnschrift" FontSize="14" Margin="0,5,0,0" Foreground="White" Width="154" HorizontalAlignment="Left"/>
                        <Border BorderBrush="#2E3235" BorderThickness="2" Background="#2E3235" CornerRadius="10" Width="200" HorizontalAlignment="Left" Height="25">
                            <TextBox x:Name="PublicTransportSystem" CaretBrush="White" FontFamily="Bahnschrift" Width="190" BorderThickness="0" Background="Transparent" Foreground="White" Padding="5" Margin="3,-2,3,-2"/>
                        </Border>
                        <Border Background="#2c3e50" BorderThickness="1" BorderBrush="#2c3e50" CornerRadius="5" Width="200" HorizontalAlignment="Left" >
                            <Button Content="Submit" FontFamily="Bahnschrift" Background="Transparent" BorderThickness="0" Foreground="LightGray" Padding="5" HorizontalAlignment="Center" Width="200" Click="PoliceReportNumberSubmit_Button_Click"/>
                        </Border>
                    </StackPanel>

                </StackPanel>

                <Grid Margin="0,246,0,0" HorizontalAlignment="Left" Width="1005">
                    <StackPanel Margin="10,1090,0,0" HorizontalAlignment="Left" Width="422" Height="250" VerticalAlignment="Top">

                        <TextBlock FontFamily="Bahnschrift" Foreground="#FAF9F7"><Run Text="Are you currently or were you previously represented by an attorney for this?"/></TextBlock>

                        <StackPanel Orientation="Horizontal">
                            <RadioButton x:Name="YesAttorneyButton" Content="Yes" FontFamily="Bahnschrift" Width="45" Checked="YesAttorneyButton_Checked" Foreground="#FAF9F7"/>
                            <RadioButton x:Name="NoAttorneyButton" Content="No" FontFamily="Bahnschrift" Width="49" Checked="NoAttorneyButton_Checked" Foreground="#FAF9F7"/>

                        </StackPanel>
                        <Border x:Name="LawyerFollowUpQuestionsBorder" Visibility="Collapsed" Height="229">
                            <StackPanel Margin="0,10,0,10">
                                <TextBlock FontFamily="Bahnschrift" Foreground="#FAF9F7"><Run Text="If yes, what was the lawyer's name/law firm?"/></TextBlock>
                                <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                    <TextBox x:Name="LawyerNameTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" />
                                </Border>
                                <TextBlock FontFamily="Bahnschrift" Foreground="#FAF9F7"><Run Text="When did you sign with them?"/></TextBlock>
                                <StackPanel Orientation="Horizontal">
                                    <DatePicker x:Name="LawyerSignDatePicker" FontFamily="Bahnschrift" SelectedDateChanged="LawyerSignDatePicker_SelectedDateChanged" Background="Transparent" BorderThickness="0" Padding="5" Width="135"/>
                                    <CheckBox x:Name="ApproxLawyerSignDate" Content="Approx?" FontFamily="Bahnschrift" VerticalAlignment="Center" Foreground="White" Visibility="Collapsed" Checked="ApproxLawyerSignDate_Checked" Unchecked="ApproxLawyerSignDate_Unchecked"/>
                                </StackPanel>
                                <TextBlock FontFamily="Bahnschrift" Foreground="#FAF9F7"><Run Text="What have they done on the case so far?"/></TextBlock>
                                <StackPanel Width="422" HorizontalAlignment="Left">
                                    <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                        <TextBox x:Name="CaseProgressTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="75"/>
                                    </Border>

                                    <Border Background="#2c3e50" BorderThickness="1" BorderBrush="#2c3e50" CornerRadius="5"  >
                                        <Button Content="Submit" FontFamily="Bahnschrift" Background="Transparent" BorderThickness="0" Foreground="LightGray" Padding="5" Margin="-1,0,-1,0"/>
                                    </Border>
                                </StackPanel>
                            </StackPanel>
                        </Border>

                    </StackPanel>

                    <StackPanel Visibility="Visible" Margin="10,1352,527,251" Width="468" Height="317">

                        <StackPanel Orientation="Horizontal" Height="199">

                            <StackPanel Orientation="Vertical" Width="216">
                                <CheckBox x:Name="AirbagsCheckBox" Content="Airbags deployed" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="AirbagsCheckBox_Checked" Unchecked="AirbagsCheckBox_Unchecked" />
                                <CheckBox x:Name="CarServiceCheckBox" Content="Involved in a car service (ex: Uber)" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="CarServiceCheckBox_Checked" Unchecked="CarServiceCheckBox_Unchecked" />
                                <CheckBox x:Name="IPWorkingCheckBox" Content="IP was working" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="IPWorkingCheckBox_Checked" Unchecked="IPWorkingCheckBox_Unchecked" />
                                <CheckBox x:Name="TeslaCheckBox" Content="Vehicle was a Tesla" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="TeslaCheckBox_Checked" Unchecked="TeslaCheckBox_Unchecked" />
                                <CheckBox x:Name="CarExplodedCheckBox" Content="Car exploded" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="CarExplodedCheckBox_Checked" Unchecked="CarExplodedCheckBox_Unchecked" />
                                <CheckBox x:Name="TireExplodedCheckBox" Content="Tire exploded" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="TireExplodedCheckBox_Checked" Unchecked="TireExplodedCheckBox_Unchecked" />
                                <CheckBox x:Name="DUIOtherDriverCheckBox" Content="DUI - Other Driver" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="DUIOtherDriverCheckBox_Checked" Unchecked="DUIOtherDriverCheckBox_Unchecked" />
                                <CheckBox x:Name="CommercialVehicleCheckBox" Content="OP was in a commercial vehicle" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="CommercialVehicleCheckBox_Checked" Unchecked="CommercialVehicleCheckBox_Unchecked" />
                                <CheckBox x:Name="SettlementOfferCheckBox" Content="Accepted a settlement offer" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Width="211" Checked="SettlementOfferCheckBox_Checked" Unchecked="SettlementOfferCheckBox_Unchecked" />
                                <CheckBox x:Name="OtherCheckBox" Content="Other/Additional Information" Margin="0,0,10,5" Foreground="White" FontFamily="Bahnschrift" Checked="OtherCheckBox_Checked" Unchecked="OtherCheckBox_Unchecked" />
                            </StackPanel>


                            <StackPanel x:Name="AirbagsMessage" Visibility="Collapsed" Width="252" Height="79">

                                <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10" Width="186">
                                    <TextBlock Text ="Cross Screen For Takata if applicable." FontSize="16" TextAlignment="Center" TextWrapping="Wrap" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" Padding="5" Height="50"/>
                                </Border>

                            </StackPanel>

                            <StackPanel x:Name="UberFUQuestions" Visibility="Collapsed" Orientation="Vertical" Margin="20,5,0,0" Width="201" Height="117">
                                <TextBlock Text="Was IP the driver or passenger?" FontFamily="Bahnschrift" Foreground="White" Margin="0,0,0,5" />

                                <StackPanel Orientation="Horizontal">
                                    <RadioButton x:Name="DriverinCarService" Content="Driver" Margin="0,0,10,0" Foreground="White" FontFamily="Bahnschrift" Checked="DriverinCarService_Checked" />
                                    <RadioButton x:Name="PassengerinCarService" Content="Passenger" Foreground="White" FontFamily="Bahnschrift" Checked="PassengerinCarService_Checked" />
                                </StackPanel>

                                <StackPanel x:Name="carServiceCompany" Visibility="Collapsed">
                                    <TextBlock Text="Car Service Company:" FontFamily="Bahnschrift" Margin="0,10,0,0" Foreground="White" Width="118" HorizontalAlignment="Left"/>
                                    <Border BorderBrush="#2E3235" BorderThickness="2" Background="#2E3235" CornerRadius="10" Width="200" HorizontalAlignment="Left" Height="25" Margin="0,5,0,0">
                                        <TextBox x:Name="IPsCarServiceCompany" CaretBrush="White" FontFamily="Bahnschrift" Width="190" BorderThickness="0" Background="Transparent" Foreground="White" Padding="5" Margin="3,-2,3,-2"/>
                                    </Border>
                                </StackPanel>
                            </StackPanel>

                            <StackPanel x:Name="IPWorkingPanel" Orientation="Vertical" Visibility="Collapsed" Height="188" Width="200" Margin="20,0,0,0">
                                <TextBlock Text="What is IP's job?" FontFamily="Bahnschrift" Foreground="White" Margin="0,10,0,5" />
                                <ComboBox x:Name="IPJobComboBox" Width="200" HorizontalAlignment="Left" FontFamily="Bahnschrift" Margin="0,0,0,10" SelectionChanged="IPJobComboBox_SelectionChanged">
                                    <ComboBoxItem Content="Delivery Driver" />
                                    <ComboBoxItem Content="Taxi Driver" />
                                    <ComboBoxItem Content="Truck Driver" />
                                    <ComboBoxItem Content="Bus Driver" />
                                    <ComboBoxItem Content="Courier" />
                                    <ComboBoxItem Content="Chauffeur" />
                                    <ComboBoxItem Content="Other" />
                                </ComboBox>
                                <StackPanel x:Name="IPOtherJobPanel" Orientation="Vertical" Visibility="Collapsed" Height="64" Width="200">
                                    <TextBlock Text="Enter Job Role:" FontFamily="Bahnschrift" Foreground="White" Margin="0,10,0,5" />
                                    <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                        <TextBox x:Name="IPOtherJobTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="24"/>
                                    </Border>
                                </StackPanel>
                                <StackPanel x:Name="IPWorkingPanel2" Orientation="Vertical" Visibility="Visible" Height="114" Width="200">
                                    <TextBlock Text="Company:" FontFamily="Bahnschrift" Foreground="White" Margin="0,0,0,5" />
                                    <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                        <TextBox x:Name="IPCompanyTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="24"/>
                                    </Border>
                                </StackPanel>

                            </StackPanel>

                            <StackPanel x:Name="CarAndTireAndTeslaAndDUIMessage" Visibility="Collapsed" Width="252" Height="79">

                                <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10" Width="186" Height="80">
                                    <TextBlock Text ="Check DOI box, make sure everything is proper." FontSize="16" TextAlignment="Center" TextWrapping="Wrap" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" Padding="5" Margin="0,4,0,11"/>
                                </Border>

                            </StackPanel>

                            <StackPanel x:Name="CommercialVehiclePanel" Orientation="Vertical" Visibility="Collapsed" Height="63" Margin="15,0,0,0">
                                <TextBlock Text="Type of Commercial Vehicle:" FontFamily="Bahnschrift" Foreground="White" Margin="0,10,0,5" />
                                <ComboBox x:Name="CommercialVehicleComboBox" Width="200" HorizontalAlignment="Left" FontFamily="Bahnschrift" Margin="0,0,0,10" SelectionChanged="CommercialVehicleComboBox_SelectionChanged">
                                    <ComboBoxItem Content="Truck" />
                                    <ComboBoxItem Content="Bus" />
                                    <ComboBoxItem Content="Taxi" />
                                    <ComboBoxItem Content="Delivery Van" />
                                </ComboBox>
                            </StackPanel>

                            <StackPanel x:Name="SettlementOfferPanel" Orientation="Vertical" Visibility="Collapsed" Height="132" Width="241">
                                <TextBlock Text="Who did you accept the settlement offer from?" FontFamily="Bahnschrift" Foreground="White" Margin="0,10,0,5" />
                                <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                    <TextBox x:Name="SettlementOfferFromTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="24"/>
                                </Border>

                                <TextBlock Text="How much was the settlement offer?" FontFamily="Bahnschrift" Foreground="White" Margin="0,0,0,5" />
                                <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                    <TextBox x:Name="SettlementOfferAmountTextBox" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="24"/>
                                </Border>
                            </StackPanel>


                        </StackPanel>

                        <StackPanel x:Name="OtherInformationStackpanel" Visibility="Collapsed" Margin="0,10,0,0">
                            <Border BorderBrush="#2E3235" Background="#2E3235" BorderThickness="2" CornerRadius="10">
                                <TextBox x:Name="OtherInformation" CaretBrush="White" FontFamily="Bahnschrift" Foreground="#FAF9F7" Background="Transparent" BorderThickness="0" Padding="5" Height="75"/>
                            </Border>

                            <Border Background="#2c3e50" BorderThickness="1" BorderBrush="#2c3e50" CornerRadius="5">
                                <Button Content="Submit" FontFamily="Bahnschrift" Background="Transparent" BorderThickness="0" Foreground="LightGray" Padding="5" Margin="-1,0,-1,0" Click="Button_Click"/>
                            </Border>
                        </StackPanel>
                    </StackPanel>


                    <StackPanel Margin="10,160,0,0" HorizontalAlignment="Left" Width="474" Height="299" VerticalAlignment="Top">
                        <StackPanel>
                            <TextBlock FontFamily="Bahnschrift"><Run Text="How did this accident happen?" Foreground="#FAF9F7"/></TextBlock>
                            <StackPanel Orientation="Horizontal">
                                <Border Background="#2c3e50" BorderThickness="1" BorderBrush="#2c3e50" CornerRadius="5" Margin="0,5,5,5" Height="20" Width="70">
                                    <Button x:Name="btnRearEnd" Content="Rear End" FontFamily="Bahnschrift" BorderThickness="0" Background="Transparent" Foreground="White" Tag="notSelected" Click="btnRearEnd_Click"/>
                                </Border>
                                <Border Background="#2c3e50" BorderThickness="1" BorderBrush="#2c3e50" CornerRadius="5" Margin="5" Height="20" Width="70">
                                    <Button x:Name="btnHeadOn" Content="Head On" FontFamily="Bahnschrift" BorderThickness="0" Background="Transparent" Foreground="White" Tag="notSelected" Click="btnHeadOn_Click" Height="18" VerticalAlignment="Top"/>
